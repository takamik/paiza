#  p "#{i}：#{e}"
#  p "#{i}：#{str[i]}"
  
  #idx = i if e == str[i]
  #break unless e == str[i]

  unless e == s[i]
   #idx = i
   #p i
   p pre + s[i..s.length]
   break
  end
end

