gets
txt = gets.strip
while s = gets
#p s
 s.strip!
 unchange_flg = true
 (0..s.length).map do |i|
#p [s[0..i], txt[txt.length-(i+1)..], txt]
  if unchange_flg && s[0..i] == txt[txt.length-(i+1)..]
   txt += s[i+1..s.length]
#p [s[0..i], txt[txt.length-(i+1)..], txt]
   unchange_flg = false
  end
 end
 txt += s if unchange_flg
end
puts txt

###################
exit

